package basics;

public class datatypes
{
	public void dataTypes()
	{
		/*
		 * data types 
		 * short 16 bit
		 * int 32 bit
		 * float 
		 * doubles 64 bit
		 * long 64 bits
		 * char
		 * string
		 * char
		 * bool
		 */
		int i =0;// Initialization
		short e;
		long l;
		float f;
		double d;
		char c;
		boolean t;
		byte bt;
	}
	pubic int add(int a ,int b)
	{
		return a+b;
		
	}
}
